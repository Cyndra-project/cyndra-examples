{
  "name": "rustcrm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run build && concurrently --names 'next, cyndra' --kill-others 'next dev' 'cargo cyndra run --working-directory ./backend/'",
    "build": "next build && cargo build --manifest-path ./backend/Cargo.toml",
    "cyndra-login": "cargo cyndra login --working-directory ./backend/",
    "full": "cargo cyndra run --working-directory backend --port 8001",
    "start": "cargo cyndra project start --working-directory ./backend/",
    "stop": "cargo cyndra project stop --working-directory ./backend/",
    "deploy": "npm run build && cargo cyndra deploy --working-directory ./backend/ --allow-dirty",
    "lint": "next lint"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "github:fortawesome/react-fontawesome",
    "@types/node": "18.15.11",
    "@types/react": "18.0.35",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "10.4.14",
    "chart.js": "^4.3.0",
    "concurrently": "^8.0.1",
    "next": "13.3.0",
    "postcss": "8.4.21",
    "react": "18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.4",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0"
  }
}
